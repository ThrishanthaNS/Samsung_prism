{
  "name": "react-typescript-tailwind",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "lucide-react": "0.542.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "tabWidth": 2,
    "printWidth": 120
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel-core": "^6.25.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "customize-cra": "1.0.0",
    "customize-cra-react-refresh": "1.1.0",
    "react-app-rewired": "2.1.6",
    "typescript": "^5.9.2",
    "webpack-bundle-analyzer": "3.8.0"
  },
  "keywords": [
    "custom-hooks",
    "tailwind",
    "typescript",
    "react"
  ],
  "description": "Template: React + TypeScript + Tailwind"
}
